{
    "name": "tryhard/image-proxy",
    "description": "A Laravel package for on-the-fly image resizing and format conversion",
    "keywords": ["laravel", "image", "proxy", "resize", "optimization"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "intervention/image-laravel": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "TryHard\\ImageProxy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TryHard\\ImageProxy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TryHard\\ImageProxy\\ImageProxyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

{
    "name": "aaronfrancis/imgproxy",
    "description": "On-the-fly image resizing and format conversion for Laravel",
    "keywords": ["laravel", "image", "proxy", "resize", "optimization"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "intervention/image-laravel": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0|^2.0|^3.0|^4.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0|^4.0"
    },
    "scripts": {
        "test": "pest",
        "lint": "pint --test",
        "fix": "pint"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\ImgProxy\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\ImgProxy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\ImgProxy\\ImgProxyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
